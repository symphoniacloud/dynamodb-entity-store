{
  "name": "@symphoniacloud/dynamodb-entity-store-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Workspace for DynamoDB Entity Store packages",
  "license": "MIT",
  "author": {
    "email": "mike@symphonia.io",
    "name": "Mike Roberts",
    "url": "https://symphonia.io"
  },
  "workspaces": [
    "packages/*",
    "examples"
  ],
  "scripts": {
    "local": "npm run local --workspace=@symphoniacloud/dynamodb-entity-store && npm run local --workspace=@symphoniacloud/dynamodb-entity-store-testutils",
    "deploy": "npm run deploy --workspace=@symphoniacloud/dynamodb-entity-store",
    "deploy-and-remote-tests": "npm run deploy-and-remote-tests --workspace=@symphoniacloud/dynamodb-entity-store",
    "check-examples": "npm run local-checks --workspace=@symphoniacloud/dynamodb-entity-store-examples",
    "generate-docs": "npm run generate-docs --workspace=@symphoniacloud/dynamodb-entity-store"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.x",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "8.x",
    "@typescript-eslint/parser": "8.x",
    "eslint": "9.x",
    "eslint-config-prettier": "9.x",
    "prettier": "3.x",
    "ts-node": "10.x",
    "typedoc": "^0.26.0",
    "typescript": "5.x",
    "typescript-eslint": "8.x",
    "vitest": "3.x"
  }
}
